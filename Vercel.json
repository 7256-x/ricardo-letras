{
  "version": 2,
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://ricardohernandezletras.com/$1",
      "permanent": true
    }
  ],
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
